{
  "editors": [
    [ "inputs::artnet1::universe 0::DMXRemap" ],
    [ "main" ]
  ],
  "inputs": {
    "artnet1": { "type": "artnet", "ip": "192.168.204.4", "universes": { "0": { "DMXRemap": { "channel": 0, "destination": "layer" } } } }
  },
  "main": {
    "layer": { "type": "layer", "resolution": [ 100, 100 ], "models": { "quad": { "effectors": [ "argb" ] } } }
  },
  "effectors": {
    "argb.glsl": "//red(0,1,1)\n//green(0,1,1)\n//blue(0,1,1)\n//alpha(0,1,1)\n \nvoid argb(float alpha, float red, float green, float  blue) { \n\n\tcolor = color * vec4 (red, green, blue, 1) * alpha; \n\n}"
  },
  "outputs": {
    "monitor": { "HDMI 1": { "type": "monitor", "resolution": [ 100, 100 ], "source": "layer" } }
  }
}